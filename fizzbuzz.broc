

fun fizzbuzz(x: int): void {
    if (x > 0) {
        if (((x % 3) == 0) && ((x % 5) == 0)) {
            print(x + ": FizzBuzz!");
        } else if ((x % 3) == 0) {
            print(x + ": Fizz")
        } else if ((x % 5) == 0) {
            print(x + ": Buzz")
        };
        fizzbuzz(x - 1);
    };
};

fizzbuzz(100);

