fun fizzbuzz(Int x): Void {
    for (Int y = 0, y < x, y = y + 1) {
        String res = "";
        if ((y % 3) == 0) {
            res = res + "Fizz";
        };
        if ((y % 5) == 0) {
            res = res + "Buzz";
        };
        if (res != "") {
            print(y + ": " + res + "!");
        }
    };
};

fizzbuzz(1000000);
