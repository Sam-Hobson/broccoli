fun fizzbuzz(Int y): Void {
    if (y > 0) {
        Int x = 1;
        while (x <= y) {
            String res = x + ": ";
            if ((x % 3) == 0) {
                res = res + "Fizz";
            };
            if ((x % 5) == 0) {
                res = res + "Buzz";
            };
            if (res != (x + ": ")) {
                res = res + "!";
                print(res);
            };

            x = x + 1;
        };
    };
};

fizzbuzz(100);
